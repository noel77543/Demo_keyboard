<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="50dp">
    <Row>
        <Key
            android:codes="33"
            android:keyEdgeFlags="left"
            android:keyLabel="\!"
            android:keyWidth="10%p" />

        <Key
            android:codes="64"
            android:keyLabel="\@"
            android:keyWidth="10%p" />

        <Key
            android:codes="35"
            android:keyLabel="\#"
            android:keyWidth="10%p" />

        <Key
            android:codes="36"
            android:keyLabel="\$"
            android:keyWidth="10%p" />
        <Key
            android:codes="37"
            android:keyLabel="\%"
            android:keyWidth="10%p" />
        <!--&-->
        <Key
            android:codes="38"
            android:keyLabel="&amp;"
            android:keyWidth="10%p" />
        <Key
            android:codes="42"
            android:keyLabel="\*"
            android:keyWidth="10%p" />
        <Key
            android:codes="59"
            android:keyLabel="\;"
            android:keyWidth="10%p" />

        <Key
            android:codes="40"
            android:keyLabel="\("
            android:keyWidth="10%p" />
        <Key
            android:codes="41"
            android:keyLabel="\)"
            android:keyWidth="10%p" />

    </Row>
    <Row>
        <Key
            android:codes="47"
            android:keyEdgeFlags="left"
            android:keyLabel="/"
            android:keyWidth="10%p" />

        <Key
            android:codes="91"
            android:keyLabel="["
            android:keyWidth="10%p" />
        <Key
            android:codes="92"
            android:keyLabel="\\"
            android:keyWidth="10%p" />
        <Key
            android:codes="93"
            android:keyLabel="]"
            android:keyWidth="10%p" />
        <Key
            android:codes="94"
            android:keyLabel="^"
            android:keyWidth="10%p" />
        <Key
            android:codes="95"
            android:keyLabel="_"
            android:keyWidth="10%p" />

        <Key
            android:codes="123"
            android:keyLabel="{"
            android:keyWidth="10%p" />
        <Key
            android:codes="124"
            android:keyLabel="|"
            android:keyWidth="10%p" />
        <Key
            android:codes="125"
            android:keyLabel="}"
            android:keyWidth="10%p" />
        <Key
            android:codes="96"
            android:keyLabel="Ë‹"
            android:keyEdgeFlags="right"
            android:keyWidth="10%p" />

    </Row>


    <Row>

        <Key
            android:codes="39"
            android:keyLabel="'"
            android:keyWidth="10%p" />


        <Key
            android:codes="61"
            android:keyLabel="\="
            android:keyWidth="10%p" />
        <Key
            android:codes="44"
            android:keyLabel="\,"
            android:keyWidth="10%p" />
        <!--<-->
        <Key
            android:codes="60"
            android:keyLabel="&lt;"
            android:keyWidth="10%p" />

        <Key
            android:codes="62"
            android:keyLabel="\>"
            android:keyWidth="10%p" />

        <Key
            android:codes="43"
            android:keyLabel="+"
            android:keyWidth="10%p" />

        <Key
            android:codes="58"
            android:keyLabel="\:"
            android:keyWidth="10%p" />

        <Key
            android:codes="63"
            android:keyLabel="\?"
            android:keyWidth="10%p" />

        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_delete"
            android:keyWidth="20%p" />
    </Row>

    <Row
        android:keyWidth="10%p"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="1000"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ic_keyboard"
            android:popupKeyboard="@xml/keys_pop_type" />
        <Key
            android:codes="32"
            android:isRepeatable="true"
            android:keyLabel="SPACE"
            android:keyWidth="55%p" />
        <Key
            android:codes="46"
            android:keyLabel="." />
        <Key
            android:codes="-4"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_enter"
            android:keyWidth="20%p" />
    </Row>
</Keyboard>